{
  "name": "stylexui",
  "displayName": "StyleXui",
  "description": "A lightweight and modern CSS framework crafted to streamline your development process. Designed with efficiency and customizability in mind, StyleXui empowers developers to create beautiful, responsive, and accessible user interfaces effortlessly.",
  "keywords": ["xui", "css", "css framework", "intellisense"],
  "publisher": "AustinefromXnyder",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "icon": "https://res.cloudinary.com/de8gwnof9/image/upload/v1738843635/vrncnazgovjxav8ki4ta.png",
  "contributes": {
    "languages": [
      {
        "id": "html",
        "extensions": [
          ".html",
          ".htm"
        ]
      },
      {
        "id": "css",
        "extensions": [
          ".css"
        ]
      },
      {
        "id": "javascript",
        "extensions": [
          ".js"
        ]
      },
      {
        "id": "javascriptreact",
        "extensions": [
          ".jsx"
        ]
      },
      {
        "id": "typescriptreact",
        "extensions": [
          ".tsx"
        ]
      }
    ],
    "configuration": {
      "title": "Autocomplete Extension",
      "properties": {
        "autocomplete.dataFilePath": {
          "type": "string",
          "default": "./suggestions.json",
          "description": "Path to the file containing suggestions"
        }
      }
    }
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:css",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "files": [
    "out",
    "suggestions.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Austinobravo/stylexui-extension.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
